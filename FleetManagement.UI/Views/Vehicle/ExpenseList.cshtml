<div class="card">
    <div class="card-body">
        <div class="d-flex mb-2 align-items-center">
            <div>
                <h5>List of fuel expenses</h5>
            </div>


        </div>

        <div class="table-responsive border rounded-1">
            <table class="table text-nowrap customize-table mb-0 align-middle">
                <thead class="text-dark fs-4">
                    <tr>
                        <th><h6 class="fs-4 fw-semibold mb-0">Amount (₦)</h6></th>
                        <th><h6 class="fs-4 fw-semibold mb-0">State</h6></th>
                        <th><h6 class="fs-4 fw-semibold mb-0">LGA</h6></th>
                        <th><h6 class="fs-4 fw-semibold mb-0">Filling Station</h6></th>
                        <th><h6 class="fs-4 fw-semibold mb-0">Date</h6></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var expense in Model)
                    {
                        <tr>
                            <td><h6 class="fs-4 fw-semibold mb-0">₦@expense.Amount.ToString("N2")</h6></td>
